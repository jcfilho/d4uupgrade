<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="sales_order_save_commit_after">
        <observer name="mk_save_partner" instance="Magebay\Marketplace\Observer\SavePartner" />
    </event>
    <event name="catalog_product_delete_after">
        <observer name="mk_delete_product" instance="Magebay\Marketplace\Observer\DeleteProduct" />
    </event>
    <event name="customer_delete_commit_after">
        <observer name="mk_delete_seller" instance="Magebay\Marketplace\Observer\DeleteSeller" />
    </event>
</config>