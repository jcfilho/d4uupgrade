<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="customer_register_success">
        <observer name="magebay.customerRegisterSuccess" instance="Magebay\Marketplace\Observer\CustomerRegisterSuccess"/>
    </event>
	<event name="controller_action_catalog_product_savepost_entity_after">
		<observer name="magebay.productsavepostafter" instance="Magebay\Marketplace\Observer\ProductSavePostAfter" />	        
    </event>
    <event name="checkout_submit_all_after">
        <observer name="magebay.saveData" instance="Magebay\Marketplace\Observer\SaveData"/>
    </event>
    <event name="sales_order_save_commit_after">
        <observer name="mk_save_partner" instance="Magebay\Marketplace\Observer\SavePartner" />
    </event>
    <event name="sales_model_service_quote_submit_success">
        <observer name="sendNewEmail" instance="Magebay\Marketplace\Observer\QuoteSubmitSuccess" />
    </event>	
</config>