<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
				<resource id="Magebay_Bookingsystem::bookingsystem" title="Booking system" sortOrder="100">
                    <resource id="Magebay_Bookingsystem::dashboard" title="Booking Dashboard" sortOrder="1" />
                    <resource id="Magebay_Bookingsystem::manage_bookings" title="Manage Bookings" sortOrder="2" />
                    <resource id="Magebay_Bookingsystem::update_booking" title="Update Booking" sortOrder="2" />
                    <resource id="Magebay_Bookingsystem::manage_facilities" title="Manage Facilities" sortOrder="3" />
                    <resource id="Magebay_Bookingsystem::add_facility" title="Add new Facility" sortOrder="4" />
                    <resource id="Magebay_Bookingsystem::manage_roomtypes" title="Manage Room Types" sortOrder="5" />
                    <resource id="Magebay_Bookingsystem::add_roomtype" title="Add New Room Type" sortOrder="6" />
                    <resource id="Magebay_Bookingsystem::manage_booking_orders" title="Booking Orders" sortOrder="7" />
                    <resource id="Magebay_Bookingsystem::configuration" title="Booking Setting" sortOrder="8" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magebay_Bookingsystem::system_config" title="Booking Setting" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>