		function selectGoingOrRoundtrip() {
			$('.product-info-main').on('change','#option-going',function(){
				checkedGoingTransfer();
                if( $(this).is(':checked') ){
                    rountripPriceUtil.changeToPriceGoing();
                }
			});
			$('.product-info-main').on('change','#option-roundtrip',function(){
				checkedRoundtripTransfer();
				if( $(this).is(':checked') ){
                    rountripPriceUtil.changeToPriceRountrip();

                }

			});
		}

		function showOptionsByTransferSelected() {
			if(isRoundTrip()){
				checkedRoundtripTransferWhenIsWishlist();
			}else{
				checkedGoingTransferWhenIsWishlist();
			}
		}

		function clearCalendarTwo(){
			$('#bk-checkin-checkout-two,#check-in-two,#check-out-two,#temp-check-in-two,#temp-check-out-two').val('').trigger('change');;
		}

		function isRoundTrip(){
			if( $('#option-roundtrip').is(':checked') ){
				return true;
			}
			return false;
		}

		selectGoingOrRoundtrip();
		showOptionsByTransferSelected();

		<?php if($moduleName == 'checkout' && $controllerName == 'cart' && $actionName == 'configure'){ ?>
