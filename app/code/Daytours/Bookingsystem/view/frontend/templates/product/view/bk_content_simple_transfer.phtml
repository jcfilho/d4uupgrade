        function nationalDays(date) {
           var m = date.month(), d = date.date() < 10 ? '0'+date.date() : date.date() , y = date.year();
            var mMonth = m+1;
            mMonth = mMonth < 10 ? '0'+mMonth : mMonth;
            var textCrrentDate = y + '-' + mMonth + '-' + d;
            //var dayObj = new Date(textCrrentDate);
            var dayObj = date.toDate();
            var qty = 0;
            if(objCalendars)
            {
                for(var key in objCalendars)
                {
                    var tempDate = objCalendars[key];
                    if(tempDate['calendar_default_value'] == '1')
                    {
                        qty = parseInt(tempDate['calendar_qty']);
                    }
                    tempStartDate = datesHelper.getDateByStringYearMonthDay(tempDate['calendar_startdate']);
                    tempEndDate = datesHelper.getDateByStringYearMonthDay(tempDate['calendar_enddate']);
                    if(tempStartDate.getTime() <= dayObj.getTime() && dayObj.getTime() <= tempEndDate.getTime())
                    {
                        if(tempDate['calendar_status'] == 'block' || tempDate['calendar_status'] == 'unavailable')
                        {
                            qty = 0;
                        }
                        else{
                            qty = parseInt(tempDate['calendar_qty']);
                        }

                    }
                }
            }
            if(objOrderItems)
            {
                for(keyorder in objOrderItems)
                {
                    var orderDate = objOrderItems[keyorder];
                    var orderStartDate = datesHelper.getDateByStringYearMonthDay(orderDate['check_in']);
                    var orderEndDate = datesHelper.getDateByStringYearMonthDay(orderDate['check_out']);
                    if(bookingTime == '2')
                    {
                        if(orderStartDate.getTime() <= dayObj.getTime() && dayObj.getTime() <= orderEndDate.getTime())
                        {
                            qty -= parseInt(orderDate['qty']);
                        }
                    }
                    else{
                        if(orderStartDate.getTime() <= dayObj.getTime() && dayObj.getTime() < orderEndDate.getTime())
                        {
                            qty -= parseInt(orderDate['qty']);
                        }
                    }

                }
            }
            if($.inArray(''+dayObj.getDay(), arDisableDay) != -1 || qty <= 0)
            {
                return true;
            }
            else
            {
                /*Disable first and next day*/
                if( !lastMinuteOperations.isProductLastMinute() ) {
                    var calendarDayBuilt = date.format('Y-M-D');
                    var currentDay = moment().tz("America/Argentina/Buenos_Aires").format('Y-M-D');
                    var calendarDayBuiltMinusOne = moment(date.toDate()).tz("America/Argentina/Buenos_Aires");
                    calendarDayBuiltMinusOne.add(-1, 'days');
                    var calendarDayBuiltMinusOneFormat = calendarDayBuiltMinusOne.format('Y-M-D');

                    if (calendarDayBuilt === currentDay || calendarDayBuiltMinusOneFormat === currentDay) {
                        return true;
                    }
                }
                let dateToWork = moment(dayObj).format('YYYY-MM-DD');
                if( $.inArray(dateToWork,objLockedDate) >= 0 ){
                    return true;
                }
                if( qty > 0 ){
                    return false;
                }
            }
        }

